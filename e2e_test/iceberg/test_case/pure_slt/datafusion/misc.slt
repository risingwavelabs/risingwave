(1, 'alice', 10),
(2, 'bob', 20),
(3, 'alice', 20),
(4, 'charlie', 30),
(5, 'bob', 20),
(6, 'dave', null),
(7, null, null),
(8, null, 10);

statement ok
FLUSH;

sleep 5s

query ? rowsort
