system ok
./risedev psql -c "
create materialized view mv as
select * from generate_series(
    to_timestamp($(date +%s)) - interval '10 second',
    now(),
    interval '1 second'
);
"

statement ok
flush;

query I
select count(*) > 10 from mv;
----
t

sleep 2s

query I
select count(*) > 12 from mv;
----
t

statement ok
drop materialized view mv;
