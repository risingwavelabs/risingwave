query TT rowsort
SELECT s.typname, t.typname
FROM pg_catalog.pg_cast as c, pg_catalog.pg_type as s, pg_catalog.pg_type as t
WHERE
	castcontext = 'IMPLICIT'
	and c.castsource = s.oid and c.casttarget = t.oid;
