query T
SELECT name
FROM
  (SELECT pg_catalog.lower(name) AS name
   FROM pg_catalog.pg_settings
   WHERE context != 'internal'
   UNION ALL SELECT 'all') ss
WHERE substring(name, 1, 7)='checkpo';
