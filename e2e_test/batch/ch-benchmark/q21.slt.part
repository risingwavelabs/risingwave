# runs, but no output. Probably need to select different rows from tables.
query TT rowsort
select	 s_name, count(*) as numwait
from	 supplier, tpcc_orderline l1, tpcc_order, tpcc_stock, nation
where	 ol_o_id = o_id
    	 and ol_w_id = o_w_id
    	 and ol_d_id = o_d_id
    	 and ol_w_id = s_w_id
    	 and ol_i_id = s_i_id
    	 and (s_w_id * s_i_id) % 10000 = s_suppkey
    	 and l1.ol_delivery_d > o_entry_d
    	 and not exists (select *
                         from	tpcc_orderline l2
                         where  l2.ol_o_id = l1.ol_o_id
                                and l2.ol_w_id = l1.ol_w_id
                                and l2.ol_d_id = l1.ol_d_id
                                and l2.ol_delivery_d > l1.ol_delivery_d)
    	 and s_nationkey = n_nationkey
    	 and n_name = 'Germany'
group by s_name
order by numwait desc, s_name;
