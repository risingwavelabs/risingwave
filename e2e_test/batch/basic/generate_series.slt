query I
SELECT * FROM generate_series('2'::INT,'10'::INT,'2'::INT)
----
2
4
6
8
10

query I
SELECT * FROM generate_series('2'::INT + '2'::INT,'10'::INT,'2'::INT);
----
4
6
8
10

query I
SELECT generate_series FROM generate_series('2'::INT + '2'::INT,'10'::INT,'2'::INT);
----
4
6
8
10